@{
    ViewData["Title"] = "Home Page";
}
<html>
<div class="text-center">
    
  <head>
    <title>Contact Form</title>
    <link rel="stylesheet" type="text/css" href="css/estilo.css">
    <!--[if IE]>
    <link rel="stylesheet" type="text/css" href="css/estiloie.css">
    <![endif]-->

  </head>
<body>
  <div id="area">
    <form id="form" autocomplete="off">
      <fieldset>
        <legend>Contact Form</legend>
        <label>Name:</label><input class="campo_nome" type="text"><br>
        <label>Email:</label><input class="campo_email" type="text"><br>
        <label>Phone:</label><input class="campo_phone" type="text"><br>
        <label>Comments:</label><br><textarea class="msg" cols="35" rows="8"></textarea><br>
        <input class="btn_submit" type="submit" value="Create">
      </fieldset>
    </form>
  </div>
</body>
</html>
</div>
